{
  "name": "@b3gm/algebr4",
  "version": "0.0.2",
  "description": "Linear algebra objects for up to 4 dimensions",
  "main": "dist/algebr4.js",
  "types": "dist/algebr4.d.ts",
  "scripts": {
    "test": "npx jest",
    "build": "npm run build:module && npm run build:debug && npm run build:prod",
    "build:prod": "webpack",
    "build:debug": "webpack -c webpack.debug.js",
    "build:module": "npx tsc -p tsconfig.module.json",
    "lint": "npx eslint . --ext .ts",
    "lint-fix": "npx eslint . --ext .ts --fix",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run lint && npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/b3gm/algebr4.git"
  },
  "author": "b3gm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/b3gm/algebr4/issues"
  },
  "homepage": "https://github.com/b3gm/algebr4#readme",
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/node": "^12.19.4",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^7.13.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.3.0",
    "ts-loader": "^8.0.11",
    "typescript": "^3.9.7",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "tsx"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": ".*\\.test\\.(ts)$",
    "collectCoverage": true
  }
}
